{
  "name": "react-race-chart",
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.(css|scss|less)$": "jest-css-modules"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "version": "1.0.0",
  "description": "A React component that renders a racing bar chart given an appropriately formatted CSV file.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "dev": "concurrently \"cd ./src/server && node server.js\" \"webpack-dev-server --config webpack.dev.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cat-in-the-mat/react-race-chart.git"
  },
  "keywords": [],
  "author": "Tolga Kurt, Rabia Arikdogan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cat-in-the-mat/react-race-chart/issues"
  },
  "homepage": "https://github.com/cat-in-the-mat/react-race-chart#readme",
  "dependencies": {
    "d3": "^5.16.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "jest-css-modules": "^2.1.0",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}